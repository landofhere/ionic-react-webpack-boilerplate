{
  "name": "ionic-react-webpack-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for Ionic React. Webpack 4, Redux 7",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config tools/webpack.config.js --env.env=dev --open --hot ",
    "build": "webpack --config tools/webpack.config.js --env.env=prod",
    "build:analyze": "npm run build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "test": "jest --runInBand --detectOpenHandles --setupFiles dotenv/config",
    "test:watch": "jest -u --runInBand --verbose --watch --detectOpenHandles --setupFiles dotenv/config",
    "test:coverage": "jest -u --coverage --verbose --runInBand --detectOpenHandles --setupFiles dotenv/config",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "keywords": [
    "ionic",
    "react",
    "webpack"
  ],
  "author": "Barry Low <barry@landofhere.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@capacitor/cli": "1.4.0",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/googlemaps": "^3.39.1",
    "@types/jest": "24.0.25",
    "@types/node": "13.1.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.1",
    "autoprefixer": "^9.7.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.6",
    "install": "^0.13.0",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-watch-typeahead": "^0.4.2",
    "lodash": "^4.17.19",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.2",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "3.7.4",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@capacitor/core": "1.4.0",
    "@ionic/react": "^4.11.7",
    "@ionic/react-router": "^4.11.7",
    "axios": "^0.19.1",
    "ionicons": "^4.6.3",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "reselect": "^4.0.0"
  }
}
